# Configurações de Ambiente para StreaminTCP-UDP
# Copie este arquivo para .env e ajuste os valores conforme necessário

# Configurações de Rede
UDP_PORT=5005
BUFFER_SIZE=65536
REDUNDANCY_FACTOR=2.5
TIMEOUT_MS=1000

# Configurações de IA/ML
ML_MODEL_PATH=./models/fec_model.h5
ENABLE_PREDICTION=true
PREDICTION_THRESHOLD=0.7

# Configurações de Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/protocol.log
ENABLE_CONSOLE_LOG=true

# Configurações de Testes
TEST_NETWORK_SIMULATION=true
SIMULATION_PACKET_LOSS_RATE=0.1
SIMULATION_LATENCY_MS=50

# Configurações de Desenvolvimento
DEBUG_MODE=true
ENABLE_PROFILING=false
PERFORMANCE_MONITORING=false

# Configurações de Integração
INTEGRATE_5G=false
INTEGRATE_IOT=true
MQTT_BROKER=localhost
MQTT_PORT=1883

# Configurações de Segurança
ENABLE_ENCRYPTION=false
ENCRYPTION_KEY=
AUTH_REQUIRED=false
API_KEY=